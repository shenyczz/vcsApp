<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="clr-namespace:Hdwas"
    xmlns:vw="clr-namespace:Hdwas.Views"
    xmlns:cskAnchor="http://schemas.csharpkit.com/windows/anchoring/xaml">

    <!--地图视图模板 DataTemplate-->
    <DataTemplate x:Key="MapViewTemplate">
        <local:MapView Map="{Binding Map,Mode=OneTime}" Background="White"/>
    </DataTemplate>

    <!--发育期管理视图模板 DataTemplate-->
    <DataTemplate x:Key="CGPViewTemplate">
        <vw:CGPView/>
    </DataTemplate>

    <DataTemplate x:Key="HdwDayViewTemplate">
        <vw:HdwDayView/>
    </DataTemplate>
    
    <DataTemplate x:Key="HdwProcessViewTemplate">
        <vw:HdwProcessView/>
    </DataTemplate>

    <DataTemplate x:Key="HdwProcessDataGridViewTemplate">
        <vw:HdwProcessDataGridView/>
    </DataTemplate>

    <DataTemplate x:Key="HdwDayDataGridViewTemplate">
        <vw:HdwDayDataGridView/>
    </DataTemplate>

    <DataTemplate x:Key="TestViewTemplate">
        <StackPanel Orientation="Vertical">
            <TextBox Text="This is test TestViewTemplate."/>
        </StackPanel>
    </DataTemplate>

</ResourceDictionary>